[core]
	excludesfile = /Users/albhaf/.gitignore_global
	editor = /usr/bin/vim
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Users/albhaf/Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[user]
	name = Albert Hafvenstr√∂m
	email = albhaf@gmail.com
[alias]
  tree = log --graph --full-history --all --color --date=short --pretty=format:\"%Cred%x09%h %Creset%ad%Cblue%d %Creset %s %C(bold)(%an)%Creset\"
  ls = "log --color=always --graph --pretty=format:'%C(yellow)%h%Creset%C(cyan)%d%Creset %C(white bold)%s%Creset %C(white dim)(by %an %ar)%Creset' --all"
  branchcommits = log --no-merges --pretty=oneline --abbrev-commit HEAD
  amend = commit --amend
  ff = merge --ff-only
  cleanup = "!git branch --merged | grep -v '^*\\|master|develop' | xargs -n 1 git branch -d"
  #cleanup = "!git branch --merged | grep -v '\*\|master\|develop' | xargs -n 1 git branch -d"
  #cleanup = "!git branch --merged | grep -v \* | grep -v master | grep -v dev | xargs -n 1 git branch -d"

[color]
	ui = auto
[http]
	cookiefile = /Users/albhaf/.gitcookies
[push]
	default = simple
[url "git@bitbucket.org:"]
	insteadOf = https://bitbucket.org/
